        <attribute name="resourceBundle"
                   value="atg.projects.store.InternationalStoreCatalogTemplateResources"/>
        <attribute name="propertySortPriority" value="-6"/>
      </property>
      <property name="descriptionDefault" data-type="string" column-name="description" 
                category-resource="categoryPresentation" display-name-resource="description">
        <attribute name="resourceBundle"
                   value="atg.projects.store.InternationalStoreCatalogTemplateResources"/>
        <attribute name="propertySortPriority" value="-8"/>
      </property>
      <property name="longDescriptionDefault" data-type="string" column-name="long_description" 
