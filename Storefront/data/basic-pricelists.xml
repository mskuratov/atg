<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE gsa-template SYSTEM "dynamosystemresource:/atg/dtds/gsa/gsa_1.0.dtd">
<gsa-template>
<import-items>
<!-- *** FOLDERS *** -->
<add-item item-descriptor="priceListFolder" id="PriceListFolder">
  <set-property name="name"><![CDATA[Price Lists]]></set-property>
  <set-property name="childItems"><![CDATA[listPrices,salePrices]]></set-property>
</add-item>

<!-- *** PRICE LISTS *** -->
<!--
These two price lists should always exist.
Don't change the repository ids of these items
-->
<add-item item-descriptor="priceList" id="listPrices">
  <set-property name="displayName"><![CDATA[List Prices]]></set-property>
</add-item>
<add-item item-descriptor="priceList" id="salePrices">
  <set-property name="displayName"><![CDATA[Sale Prices]]></set-property>
</add-item>
</import-items>
</gsa-template>
