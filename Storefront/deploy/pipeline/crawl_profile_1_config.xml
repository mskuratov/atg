<!DOCTYPE CRAWL_PROFILE_CONFIG SYSTEM "crawl_profile_config.dtd">

<CRAWL_PROFILE_CONFIG STATUS="ENABLED">
  <MAX_DEPTH VALUE="5"/>
  <MAX_REQUESTS VALUE="10000"/>
  <MAX_PAGE_SIZE VALUE="20"/>
  <USER_AGENT VALUE="Endeca Crawler"/>
  <USE_COOKIES VALUE="TRUE"/>
  <OBEY_ROBOTS_TXT VALUE="TRUE"/>
  <REQUEST_DELAY VALUE="50"/>
  <BYPASS_PROXY_SERVER VALUE="FALSE"/>
  <ENABLE_HTTP_PIPELINING VALUE="TRUE"/>
  <HTTP_PIPELINE_SIZE VALUE="6"/>
  <CONCURRENT_SPIDERS VALUE="300"/>
  <FETCHER_THREADS VALUE="30"/>
  <PARSING_THREADS VALUE="10"/>
  <PROCESSING_THREADS VALUE="10"/>
  <ENABLE_INCREMENTAL_CRAWLING VALUE="FALSE"/>
  <INCREMENTAL_CRAWL_CACHE_DIRECTORY VALUE=""/>
  <CRAWL_SCOPE VALUE="ANY"/>
  <URL_FILTERS/>
</CRAWL_PROFILE_CONFIG>
