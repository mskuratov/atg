<%-- 
  This file is used for display item id, and it should be included in the pickerLayout.jspf file directly.
--%>
<dsp:getvalueof var="selectedSku" param="selectedSku"/>
<c:if test="${not empty selectedSku}">
  <span class="atg_store_itemID">
    <dsp:valueof param="selectedSku.id" />
  </span>
</c:if>
